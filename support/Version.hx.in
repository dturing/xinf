package xinf;

class Version {
	public static var version:String = "__VERSION__";
	public static var revision:String = "__REVISION__";
	public static var built:String = "__DATE__";
	public static var tagline:String = "__TAGLINE__";
	
	public static function fullVersion() :String {
		return( "Xinf "+version+" '"+tagline+"' r"+revision+" built "+built );
	}
}
